<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/bg1"
    tools:context=".MainActivity">

    <ImageView
        android:layout_width="133dp"
        android:layout_height="42dp"
        android:src="@drawable/sign_up"
        android:layout_centerInParent="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.139"
        tools:ignore="ContentDescription,HardcodedText" />


    <ImageView
        android:id="@+id/textView"
        android:layout_width="280dp"
        android:layout_height="19dp"
        android:src="@drawable/signup_title"
        android:text="대충 앱 이름"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.226"
        tools:ignore="ContentDescription,HardcodedText,MissingConstraints" />

    <ImageButton
        android:id="@+id/myButton"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:src="@drawable/button_si"
        android:text="회원가입"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647"
        tools:ignore="ContentDescription,HardcodedText,MissingConstraints,RtlHardcoded" />

    <ImageButton
        android:id="@+id/button2"
        android:layout_width="34dp"
        android:layout_height="12dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:src="@drawable/switchtologin"
        android:text="로그인"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.673"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.729"
        tools:ignore="ContentDescription,HardcodedText" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="122dp"
        android:layout_height="14dp"
        android:drawableLeft="@drawable/swtichtologintext"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.729"
        tools:ignore="HardcodedText,MissingConstraints,RtlHardcoded,UseCompatTextViewDrawableXml" />

    <EditText
        android:id="@+id/textView4"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:background="@drawable/separator_username"
        android:hint="비밀번호"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:textSize="12sp"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        tools:ignore="HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/textView3"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:background="@drawable/separator_username"
        android:hint="사용자 이름"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.428"
        tools:ignore="HardcodedText,MissingConstraints,TextFields" />

    <EditText
        android:id="@+id/textView2"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:background="@drawable/separator_imail"
        android:hint="이메일"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.358"
        tools:ignore="HardcodedText,MissingConstraints,TextFields" />

</androidx.constraintlayout.widget.ConstraintLayout>